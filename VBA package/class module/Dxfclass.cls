Sub dxfRect(X_pos As Double, Y_pos As Double, Width As Double, Height As Double)
    Call dxfLine(X_pos, Y_pos, X_pos, Y_pos + Height)
    Call dxfLine(X_pos, Y_pos + Height, X_pos + Width, Y_pos + Height)
    Call dxfLine(X_pos + Width, Y_pos + Height, X_pos + Width, Y_pos)
    Call dxfLine(X_pos + Width, Y_pos, X_pos, Y_pos)
End Sub
Sub dxfCircle(radius As Double, Pos_X As Double, Pos_Y As Double)
    Open save_path For Append As #1
        Print #1, "0"
        Print #1, "CIRCLE"
        Print #1, "8"
        Print #1, "_0-1_"
        Print #1, "6"
        Print #1, "CONTINUOUS"
        Print #1, "62"
        Print #1, "7"
        Print #1, "10"
        Print #1, Pos_X
        Print #1, "20"
        Print #1, Pos_Y
        Print #1, "40"
        Print #1, radius
    Close #1
End Sub
Sub dxfText(X_pos As Double, Y_pos As Double, str_size As Double, width_scale As Double, degree As Double, str As String)
